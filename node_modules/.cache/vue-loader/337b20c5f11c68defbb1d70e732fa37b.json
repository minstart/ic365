stats.downloaded }}</div>\n            </div>\n          </el-card>\n        </el-col>\n        <el-col :span=\"6\">\n          <el-card shadow=\"hover\">\n            <div class=\"stat-card\">\n              <div class=\"stat-title\">已分离</div>\n              <div class=\"stat-value\">{{ stats.separate }}</div>\n            </div>\n          </el-card>\n        </el-col>\n        <el-col :span=\"6\">\n          <el-card shadow=\"hover\">\n            <div class=\"stat-card\">\n              <div class=\"stat-title\">已合并</div>\n              <div class=\"stat-value\">{{ stats.combine }}</div>\n            </div>\n          </el-card>\n        </el-col>\n      </el-row>\n\n      <!-- 搜索和筛选 -->\n      <el-form :inline=\"true\" class=\"demo-form-inline\">\n        <el-form-item label=\"状态筛选\">\n          <el-select v-model=\"filterStatus\" placeholder=\"请选择状态\">\n            <el-option label=\"全部\" value=\"all\" />\n            <el-option label=\"待处理\" value=\"pending\" />\n            <el-option label=\"已下载\" value=\"downloaded\" />\n            <el-option label=\"已分离\" value=\"separate\" />\n            <el-option label=\"已合并\" value=\"combine\" />\n          </el-select>\n        </el-form-item>\n\n        <el-form-item label=\"搜索\">\n          <el-input v-model=\"searchQuery\" placeholder=\"输入标题或内容\" />\n        </el-form-item>\n\n        <el-form-item>\n          <el-button type=\"primary\" @click=\"fetchData\">查询</el-button>\n        </el-form-item>\n      </el-form>\n\n      <!-- 视频表格 -->\n      <el-table\n        v-loading=\"listLoading\"\n        :data=\"list\"\n        border\n        fit\n        highlight-current-row\n        style=\"width: 100%\"\n      >\n        <el-table-column label=\"ID\" prop=\"id\" width=\"80\" align=\"center\" />\n        <el-table-column label=\"标题\" prop=\"title\" min-width=\"200\" />\n\n        <el-table-column label=\"已下载\" width=\"100\" align=\"center\">\n          <template slot-scope=\"{row}\">\n            <el-tag :type=\"row.downloaded ? 'success' : 'danger'\">\n              {{ row.downloaded ? '是' : '否' }}\n            </el-tag>\n          </template>\n        </el-table-column>\n\n        <el-table-column label=\"已分离\" width=\"100\" align=\"center\">\n          <template slot-scope=\"{row}\">\n            <el-tag :type=\"row.separate ? 'success' : 'danger'\">\n              {{ row.separate ? '是' : '否' }}\n            </el-tag>\n          </template>\n        </el-table-column>\n\n        <el-table-column label=\"已合并\" width=\"100\" align=\"center\">\n          <template slot-scope=\"{row}\">\n            <el-tag :type=\"row.combine ? 'success' : 'danger'\">\n              {{ row.combine ? '是' : '否' }}\n            </el-tag>\n          </template>\n        </el-table-column>\n\n        <el-table-column label=\"操作\" width=\"220\" align=\"center\">\n          <template slot-scope=\"{row}\">\n            <el-button\n              v-if=\"row.separate\"\n              type=\"primary\"\n              size=\"mini\"\n              @click=\"handleDownloadBundle(row)\"\n            >\n              下载资源\n            </el-button>\n\n            <el-button\n              v-if=\"row.separate && !row.combine\"\n              type=\"success\"\n              size=\"mini\"\n              @click=\"handleCombine(row)\"\n            >\n              上传合并\n            </el-button>\n          </template>\n        </el-table-column>\n      </el-table>\n\n      <Pagination\n        :total=\"total\"\n        :current-page.sync=\"listQuery.page\"\n        :page-size.sync=\"listQuery.limit\"\n        @pagination-change=\"fetchData\"\n      />\n    </el-card>\n  </div>\n</template>\n\n<script>\nimport { getVideoList, getVideoStats, handleCombine, handleDownloadBundle } from '@/api/video'\nimport Pagination from '@/components/Pagination'\n\nexport default {\n  name: 'VideoList',\n  components: { Pagination }, // 注册组件\n  data() {\n    return {\n      list: [],\n      total: 0,\n      listLoading: true,\n      listQuery: {\n        page: 1,\n        limit: 10\n      },\n      filterStatus: 'all',\n      searchQuery: '',\n      stats: {\n        totalVideos: 0,\n        downloaded: 0,\n        separate: 0,\n        combine: 0\n      }\n    }\n  },\n  created() {\n    this.fetchData()\n    this.fetchStats()\n  },\n  methods: {\n    handleDownloadBundle,\n    handleCombine,\n    fetchData() {\n      this.listLoading = true\n\n      getVideoList({\n        page: this.listQuery.page,\n        size: this.listQuery.limit,\n        status: this.filterStatus,\n        search: this.searchQuery\n      }).then(response => {\n        this.list = response.data.content\n        this.total = response.data.totalElements\n        this.listLoading = false\n      })\n    },\n\n    fetchStats() {\n      getVideoStats().then(response => {\n        this.stats = response.data\n      })\n    }\n  }\n}\n</script>\n\n<style scoped>\n.stat-card {\n  text-align: center;\n  padding: 10px 0;\n}\n\n.stat-title {\n  font-size: 14px;\n  color: #909399;\n  margin-bottom: 5px;\n}\n\n.stat-value {\n  font-size: 24px;\n  font-weight: bold;\n}\n</style>\n"]}]}