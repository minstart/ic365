ceholder=\"选择日期\"\n            format=\"yyyy-MM-dd\"\n            value-format=\"yyyy-MM-dd\"\n          />\n        </el-form-item>\n\n        <el-form-item>\n          <el-button type=\"primary\" @click=\"fetchData\">查询</el-button>\n        </el-form-item>\n      </el-form>\n\n      <!-- 题目表格 -->\n      <el-table :data=\"list\" border fit highlight-current-row>\n        <el-table-column label=\"ID\" prop=\"questionId\" width=\"200\" align=\"center\" />\n        <el-table-column label=\"标题\" prop=\"title\" min-width=\"150\" />\n\n        <el-table-column label=\"类型\" prop=\"typeText\" width=\"100\" align=\"center\" />\n        <el-table-column label=\"状态\" prop=\"statusText\" width=\"80\" align=\"center\" />\n        <el-table-column label=\"年级\" prop=\"gradeText\" width=\"80\" align=\"center\" />\n        <el-table-column label=\"分类\" prop=\"categoryName\" width=\"120\" align=\"center\" />\n        <el-table-column label=\"创建时间\" prop=\"createTime\" width=\"180\" align=\"center\" />\n\n        <el-table-column label=\"操作\" width=\"120\" align=\"center\">\n          <template slot-scope=\"{row}\">\n            <el-button\n              type=\"primary\"\n              size=\"mini\"\n              @click=\"handleEdit(row.questionId)\"\n            >\n              编辑\n            </el-button>\n          </template>\n        </el-table-column>\n      </el-table>\n\n      <Pagination\n        :total=\"total\"\n        :current-page.sync=\"listQuery.page\"\n        :page-size.sync=\"listQuery.size\"\n        @pagination-change=\"fetchData\"\n      />\n    </el-card>\n  </div>\n</template>\n\n<script>\nimport { getQuestionList } from '@/api/question'\nimport Pagination from '@/components/Pagination'\n\nexport default {\n  name: 'QuestionList',\n  components: { Pagination },\n  data() {\n    return {\n      list: [],\n      total: 0,\n      listLoading: true,\n      listQuery: {\n        page: 1,\n        size: 10\n      },\n      searchTitle: '',\n      filterStatus: '',\n      pushDate: ''\n    }\n  },\n  created() {\n    this.fetchData()\n  },\n  methods: {\n    fetchData() {\n      getQuestionList({\n        page: this.listQuery.page,\n        size: this.listQuery.size,\n        title: this.searchTitle,\n        disabled: this.filterStatus,\n        pushDateStr: this.pushDate\n      }).then(response => {\n        this.list = response.data.content\n        this.total = response.data.totalElements\n      })\n    },\n\n    handleEdit(id) {\n      console.log(`/question/edit/${id}`)\n      this.$router.push(`/question/edit/${id}`)\n    }\n  }\n}\n</script>\n"]}]}