: crypto,\n    encrypt_enabled: process.env.VUE_APP_ENCRYPT_ENABLED === 'true'\n  }\n}\n\nconst state = getDefaultState()\n\nconst mutations = {\n  RESET_STATE: (state) => {\n    sessionStorage.removeItem('crypto')\n    Object.assign(state, getDefaultState())\n  },\n  SET_TOKEN: (state, token) => {\n    state.token = token\n  },\n  SET_NAME: (state, name) => {\n    state.name = name\n  },\n  SET_AVATAR: (state, avatar) => {\n    state.avatar = avatar\n  },\n  SET_ROLES: (state, roles) => {\n    state.roles = roles\n  },\n  SET_MENUS: (state, menus) => {\n    state.menus = menus\n  },\n  SET_CRYPTO: (state, crypto) => {\n    state.crypto = crypto\n    sessionStorage.setItem('crypto', JSON.stringify(crypto))\n  },\n  SET_ENCRYPTENABLED: (state, encryptEnabled) => {\n    state.encryptEnabled = encryptEnabled\n  }\n}\n\nconst actions = {\n  // user login\n  login({ commit }, userInfo) {\n    const { username, password } = userInfo\n    return new Promise((resolve, reject) => {\n      login({ username: username.trim(), password: password }).then(response => {\n        const { data } = response\n        console.log(data)\n        commit('SET_TOKEN', data.token)\n        setToken(data.token)\n        resolve()\n      }).catch(error => {\n        reject(error)\n      })\n    })\n  },\n\n  // get user info\n  getInfo({ commit, state }) {\n    return new Promise((resolve, reject) => {\n      getInfo(state.token).then(response => {\n        const { data } = response\n\n        if (!data) {\n          reject('Verification failed, please Login again.')\n        }\n\n        const { roles, name, avatar } = data\n\n        // roles must be a non-empty array\n        if (!roles || roles.length <= 0) {\n          reject('getInfo: roles must be a non-null array!')\n        }\n\n        commit('SET_ROLES', roles)\n        commit('SET_NAME', name)\n        commit('SET_AVATAR', avatar)\n        resolve(data)\n      }).catch(error => {\n        reject(error)\n      })\n    })\n  },\n\n  // user logout\n  logout({ commit, state }) {\n    return new Promise((resolve, reject) => {\n      logout(state.token).then(() => {\n        removeToken() // must remove  token  first\n        resetRouter()\n        commit('RESET_STATE')\n        resolve()\n      }).catch(error => {\n        reject(error)\n      })\n    })\n  },\n\n  // remove token\n  resetToken({ commit }) {\n    return new Promise(resolve => {\n      removeToken() // must remove  token  first\n      commit('RESET_STATE')\n      resolve()\n    })\n  },\n  getMenuFromServer({ commit }) {\n    return new Promise((resolve, reject) => {\n      getMenu().then(response => {\n        const menus = response.data\n        commit('SET_MENUS', menus)\n        resolve(menus)\n      }).catch(error => {\n        reject(error)\n      })\n    })\n  }\n}\n\nexport default {\n  namespaced: true,\n  state,\n  mutations,\n  actions\n}\n"],"mappings":";;;;;AAAA,SAASA,KAAK,IAALA,MAAK,EAAEC,MAAM,IAANA,OAAM,EAAEC,OAAO,IAAPA,QAAO,EAAEC,OAAO,QAAQ,YAAY;AAC5D,SAASC,QAAQ,EAAEC,QAAQ,EAAEC,WAAW,QAAQ,cAAc;AAC9D,SAASC,WAAW,QAAQ,UAAU;AAEtC,IAAMC,eAAe,GAAG,SAAlBA,eAAeA,CAAA,EAAS;EAE5B;EACA,IAAMC,MAAM,GAAGC,IAAI,CAACC,KAAK,CAACC,cAAc,CAACC,OAAO,CAAC,QAAQ,CAAC,IAAI,MAAM,CAAC,IAAI;IACvEC,UAAU,EAAE,EAAE;IACdC,MAAM,EAAE,EAAE;IACVC,UAAU,EAAE;EACd,CAAC;EAED,OAAO;IACLC,KAAK,EAAEb,QAAQ,CAAC,CAAC;IACjBc,IAAI,EAAE,EAAE;IACRC,MAAM,EAAE,EAAE;IACVC,KAAK,EAAE,EAAE;IACTC,KAAK,EAAE,EAAE;IACTZ,MAAM,EAAEA,MAAM;IACda,eAAe,EAAEC,OAAO,CAACC,GAAG,CAACC,uBAAuB,KAAK;EAC3D,CAAC;AACH,CAAC;AAED,IAAMC,KAAK,GAAGlB,eAAe,CAAC,CAAC;AAE/B,IAAMmB,SAAS,GAAG;EAChBC,WAAW,EAAE,SAAbA,WAAWA,CAAGF,KAAK,EAAK;IACtBd,cAAc,CAACiB,UAAU,CAAC,QAAQ,CAAC;IACnCC,MAAM,CAACC,MAAM,CAACL,KAAK,EAAElB,eAAe,CAAC,CAAC,CAAC;EACzC,CAAC;EACDwB,SAAS,EAAE,SAAXA,SAASA,CAAGN,KAAK,EAAET,KAAK,EAAK;IAC3BS,KAAK,CAACT,KAAK,GAAGA,KAAK;EACrB,CAAC;EACDgB,QAAQ,EAAE,SAAVA,QAAQA,CAAGP,KAAK,EAAER,IAAI,EAAK;IACzBQ,KAAK,CAACR,IAAI,GAAGA,IAAI;EACnB,CAAC;EACDgB,UAAU,EAAE,SAAZA,UAAUA,CAAGR,KAAK,EAAEP,MAAM,EAAK;IA