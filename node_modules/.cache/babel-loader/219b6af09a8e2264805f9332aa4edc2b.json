l>\n        <el-col :span=\"6\">\n          <el-card shadow=\"hover\">\n            <div class=\"stat-card\">\n              <div class=\"stat-title\">已下载</div>\n              <div class=\"stat-value\">{{ stats.downloaded }}</div>\n            </div>\n          </el-card>\n        </el-col>\n        <el-col :span=\"6\">\n          <el-card shadow=\"hover\">\n            <div class=\"stat-card\">\n              <div class=\"stat-title\">已分离</div>\n              <div class=\"stat-value\">{{ stats.separate }}</div>\n            </div>\n          </el-card>\n        </el-col>\n        <el-col :span=\"6\">\n          <el-card shadow=\"hover\">\n            <div class=\"stat-card\">\n              <div class=\"stat-title\">已合并</div>\n              <div class=\"stat-value\">{{ stats.combine }}</div>\n            </div>\n          </el-card>\n        </el-col>\n      </el-row>\n\n      <!-- 搜索和筛选 -->\n      <el-form :inline=\"true\" class=\"demo-form-inline\">\n        <el-form-item label=\"状态筛选\">\n          <el-select v-model=\"filterStatus\" placeholder=\"请选择状态\">\n            <el-option label=\"全部\" value=\"all\" />\n            <el-option label=\"待处理\" value=\"pending\" />\n            <el-option label=\"已下载\" value=\"downloaded\" />\n            <el-option label=\"已分离\" value=\"separate\" />\n            <el-option label=\"已合并\" value=\"combine\" />\n          </el-select>\n        </el-form-item>\n\n        <el-form-item label=\"搜索\">\n          <el-input v-model=\"searchQuery\" placeholder=\"输入标题或内容\" />\n        </el-form-item>\n\n        <el-form-item>\n          <el-button type=\"primary\" @click=\"fetchData\">查询</el-button>\n        </el-form-item>\n      </el-form>\n\n      <!-- 视频表格 -->\n      <el-table\n        v-loading=\"listLoading\"\n        :data=\"list\"\n        border\n        fit\n        highlight-current-row\n        style=\"width: 100%\"\n      >\n        <el-table-column label=\"ID\" prop=\"id\" width=\"80\" align=\"center\" />\n        <el-table