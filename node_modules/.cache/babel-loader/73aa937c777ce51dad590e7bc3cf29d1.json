\n    }\n  })\n}\n\nexport function getVideoStats() {\n  return request({\n    url: '/video/stats',\n    method: 'get'\n  })\n}\n\nexport function handleCombine(row) {\n  // 创建文件输入元素\n  const input = document.createElement('input')\n  input.type = 'file'\n  input.accept = 'video/*' // 限制只选择视频文件\n  input.style.display = 'none'\n\n  // 添加文件选择事件监听\n  input.onchange = async (e) => {\n    const file = e.target.files[0]\n    if (!file) return\n    try {\n      this.listLoading = true\n      const formData = new FormData()\n      formData.append('file', file) // 添加文件\n      formData.append('id', row.id) // 添加视频ID\n\n      // 从本地存储获取token\n      const token = getToken() || ''\n\n      // 发送上传请求\n      const response = await axios.post('/admin/video/uploadCombine', formData, {\n        headers: {\n          'Authorization': `Bearer ${token}`,\n          'Content-Type': 'multipart/form-data'\n        }\n      })\n\n      this.$message.success('视频上传成功！')\n      this.fetchData() // 刷新数据\n    } catch (error) {\n      console.error('上传失败:', error)\n      this.$message.error('视频上传失败: ' + (error.response?.data?.message || error.message))\n    } finally {\n      this.listLoading = false\n      document.body.removeChild(input) // 清理DOM元素\n    }\n  }\n\n  // 触发文件选择\n  document.body.appendChild(input)\n  input.click()\n}\n\nexport async function handleDownloadBundle(row) {\n  const loadingInstance = this.$loading({\n    lock: true,\n    text: '资源包下载中...',\n    spinner: 'el-icon-loading',\n    background: 'rgba(0, 0, 0, 0.7)'\n  });\n  try {\n\n    const token = getToken()\n\n    // 发起带授权的下载请求\n    const response = await fetch(`/admin/video/download-bundle/${row.id}`, {\n      headers: {\n        'Authorization': `Bearer ${token}` // 根据后端要求调整格式\n      }\n    })\n\n    if (!response.ok) throw new Error('下载失败: ' + response.status)\n\n    // 将响应转换为Blob\n    const blob = await response.blob()\n    const url = URL.createObjectURL(blob)\n\n    // 创建下载链接\n    const downloadLink = document.createElement('a')\n    downloadLink.href = url\n    downloadLink.download = `视频分离资源包_${row.id}.zip`\n    downloadLink.style.display = 'none'\n\n    // 触发下载\n    document.body.appendChild(downloadLink)\n    downloadLink.click()\n\n    // 清理资源\n    setTimeout(() => {\n      document.body.removeChild(downloadLink)\n      URL.revokeObjectURL(url) // 释放内存\n    }, 100)\n\n    this.$notify({\n      title: '开始下载',\n      message: `视频分离资源包_${row.id}.zip 开始下载`,\n      type: 'success',\n      duration: 3000\n    })\n\n  } catch (error) {\n    console.error('下载错误:', error)\n    this.$notify({\n      title: '下载失败',\n      message: error.message || '文件下载失败',\n      type: 'error',\n      duration: 5000\n    })\n  } finally {\n    loadingInstance.close()\n  }\n}\n"],"mappings":";;;;;;;;;;AAAA,OAAOA,OAAO,MAAM,iBAAiB;AACrC,SAASC,QAAQ,QAAQ,cAAc;AACvC,OAAOC,KAAK,MAAM,OAAO;AAEzB,OAAO,SAASC,YAAYA,CAACC,MAAM,EAAE;EACnC,OAAOJ,OAAO,CAAC;IACbK,GAAG,EAAE,gBAAgB;IACrBC,MAAM,EAAE,KAAK;IACbF,MAAM,EAAE;MACNG,IAAI,EAAEH,MAAM,CAACG,IAAI;MACjBC,IAAI,EAAEJ,MAAM,CAACI,IAAI;MACjBC,MAAM,EAAEL,MAAM,CAACK,MAAM;MACrBC,MAAM,EAAEN,MAAM,CAACM;IACjB;EACF,CAAC,CAAC;AACJ;AAEA,OAAO,SAASC,aAAaA,CAAA,EAAG;EAC9B,OAAOX,OAAO,CAAC;IACbK,GAAG,EAAE,cAAc;IACnBC,MAAM,EAAE;EACV,CAAC,CAAC;AACJ;AAEA,OAAO,SAASM,aAAaA,CAACC,GAAG,EAAE;EAAA,IAAAC,KAAA;EACjC;EACA,IAAMC,KAAK,GAAGC,QAAQ,CAACC,aAAa,CAAC,OAAO,CAAC;EAC7CF,KAAK,CAACG,IAAI,GAAG,MAAM;EACnBH,KAAK,CAACI,MAAM,GAAG,SAAS,EAAC;EACzBJ,KAAK,CAACK,KAAK,CAACC,OAAO,GAAG,MAAM;;EAE5B;EACAN,KAAK,CAACO,QAAQ;IAAA,IAAAC,IAAA,GAAAC,iBAAA,cAAAC,YAAA,GAAAC,CAAA,CAAG,SAAAC,QAAOC,CAAC;MAAA,IAAAC,IAAA,EAAAC,QAAA,EAAAC,KAAA,EAAAC,QAAA,EAAAC,eAAA,EAAAC,EAAA;MAAA,OAAAT,YAAA,GAAAU,CAAA,WAAAC,QAAA;QAAA,kBAAAA,QAAA,CAAAC,CAAA;UAAA;YACjBR,IAAI,GAAGD,CAAC,CAACU,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;YAAA,IACzBV,IAAI;cAAAO,QAAA,CAAAC,CAAA;cAAA;YAAA;YAAA,OAAAD,QAAA,CAAAI,CAAA;UAAA;YAAAJ,QAAA,CAAAK,CAAA;YAEP3B,KAAI,CAAC4B,WAAW,GAAG,IAAI;YACjBZ,QAAQ,GAAG,IAAIa,QAAQ,CAAC,CAAC;YAC/Bb,QAAQ,CAACc,MAAM,CAAC,MAAM,EAAEf,IAA